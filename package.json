{
  "name": "sails-redis-schema",
  "version": "1.0.0",
  "description": "A redis adapter for Sails / Waterline with basic schema support",
  "scripts": {
    "test": "node test",
    "pretest": "npm run lint",
    "lint": "standard --fix",
    "standard": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/miroculus/sails-redis-schema.git"
  },
  "keywords": [
    "redis",
    "hash",
    "schema",
    "adapter",
    "sails",
    "sails.js",
    "waterline",
    "orm",
    "database-adapter"
  ],
  "author": "mjlescano",
  "license": "MIT",
  "dependencies": {
    "ioredis": "4.0.0"
  },
  "devDependencies": {
    "pre-commit": "1.2.2",
    "standard": "12.0.1",
    "waterline-adapter-tests": "1.0.1"
  },
  "waterlineAdapter": {
    "type": "sails-redis-schema",
    "waterlineVersion": "^0.13.0",
    "interfaces": [
      "semantic",
      "queryable",
      "associations"
    ],
    "features": []
  },
  "precommit.silent": false,
  "pre-commit": [
    "standard"
  ]
}
